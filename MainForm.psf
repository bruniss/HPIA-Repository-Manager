# convert a date object to an 8601 string
function ISO8601DateString
{
	[CmdletBinding()]
	param (
		[datetime]$Date
	)
	$Date.ToString("yyyy-MM-dd'T'HH:mm:ss.fffffff", [System.Globalization.CultureInfo]::InvariantCulture)
}

